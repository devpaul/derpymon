    "chai": "^4.1.2",
    "intern": "^4.1.4",
    "ip": "^1.1.5",
    "opn": "^5.1.0",
    "sinon": "^2.0.0",
    "ts-node": "^4.0.2",
    "tslint-sitepen": "^0.1.0",
    "typescript": "^2.6.2",
