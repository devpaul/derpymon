    "@dojo/shim": "^0.2.3",
    "@dojo/widget-core": "^0.1.0",
    "@types/aframe": "^0.5.0",
    "@webcomponents/custom-elements": "^1.0.2",
    "@webcomponents/html-imports": "^1.0.1",
    "aframe": "^0.7.0",
